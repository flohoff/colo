	__stack = .;

	.text :
	{
		start.o(.text)
		*(.text)
		*(.rodata*)
		__stage2 = .;
		*(.stage2)
	}

	.data :
	{
		*(.data)
	}

	.bss :	
	{
		__bss = .;
