__stack = __data;

SECTIONS
{
	. = 0x80080000;

	.data : AT(__etext)
	{
		__data = .;
		*(.data)
		__edata = .;
	}

	.bss :	
	{
		__bss = .;
		*(.scommon)
		_gp = .;
		*(.sbss)
		*(COMMON)
		*(.bss)
		LONG(0);	/* "strip" doesn't like empty .bss */
		__ebss = .;
	}

	.text :
	{
		__text = .;
		start.o(.text)
		*(.text)
		*(.rodata)
		__stage2 = .;
		*(.stage2)
		__etext = .;
	}

	/DISCARD/ :
	{
		*(.reginfo)
	}
}

ENTRY(_start);
