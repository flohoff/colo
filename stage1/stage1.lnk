__stack = 0x80100000;

SECTIONS
{
	. = __stack - 0x4000;

	.data : AT(__etext)
	{
		__data = .;
		*(.data)
		__edata = .;
	}

	.bss :	
	{
		__bss = .;
		*(.scommon)
		_gp = .;
		*(.sbss)
		*(COMMON)
		*(.bss)
		__ebss = .;
	}


	. = 0xbfc00000;

	.text :
	{
		__text = .;
		start.o(.text)
		*(.text)
		*(.rodata)
		__stage2 = .;
		*(.stage2)
		__etext = .;
	}

	/DISCARD/ :
	{
		*(.reginfo)
	}
}
