__stack = __ebss + (32 << 10);

SECTIONS
{
	. = 0x80080000;

	.text :
	{
		__executable_start = .;
		*(.text)
		*(.rodata*)
	}

	.data :
	{
		*(.data)
	}

	.bss :	
	{
		__bss = .;
		*(.scommon)
		_gp = .;
		*(.sbss)
		*(COMMON)
		*(.bss)
		__ebss = .;
	}

	/DISCARD/ :
	{
		*(.reginfo)
	}
}

ENTRY(_start);
